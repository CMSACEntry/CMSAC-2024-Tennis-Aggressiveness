    "def add_player_shot_column(df):\n",
    "    match_components = df['match_id'].str.split('-', expand=True)\n",
    "    df['Player_1'] = match_components.iloc[:, -2]  # Second to last component\n",
    "    df['Player_2'] = match_components.iloc[:, -1]  # Last component\n",
    "    \n",
    "    df['Player_1'] = df['Player_1'].str.replace('_', ' ')\n",
    "    df['Player_2'] = df['Player_2'].str.replace('_', ' ')\n",
    "\n",
    "    df['Hitting_Player'] = None\n",
    "    \n",
    "    current_server = None\n",
    "    current_shot_num = None\n",
    "\n",
    "    for index, row in df.iterrows():\n",
    "        if row['shot_num'] == 1:\n",
    "            current_server = row['Svr']\n",
    "            current_shot_num = 1\n",
    "        \n",
    "        if current_shot_num % 2 == 1:\n",
    "            hitting_player = df.at[index, 'Player_1'] if current_server == 1 else df.at[index, 'Player_2']\n",
    "        else:\n",
    "            hitting_player = df.at[index, 'Player_2'] if current_server == 1 else df.at[index, 'Player_1']\n",
    "        \n",
    "        df.at[index, 'Hitting_Player'] = hitting_player\n",
    "        \n",
    "        current_shot_num += 1\n",
    "    \n",
    "    return df"
   ]
