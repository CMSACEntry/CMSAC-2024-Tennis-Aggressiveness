    "def add_identifiers(df):\n",
    "    # Initialize the columns\n",
    "    df['pt_id'] = None\n",
    "    df['gm_id'] = None\n",
    "    df['set_id'] = None\n",
    "\n",
    "    # Variables to keep track of current set and game\n",
    "    current_set = None\n",
    "    current_match = None\n",
    "\n",
    "    # Initialize dictionaries to track set and game identifiers\n",
    "    set_ids = {}\n",
    "    game_ids = {}\n",
    "\n",
    "    for idx, row in df.iterrows():\n",
    "        match_id = row['match_id']\n",
    "        pt_number = row['Pt']\n",
    "        game_number = row['Gm#'][0]  # Assuming Gm# is a list, make sure this is correct\n",
    "        \n",
    "        # Update current match if changed\n",
    "        if match_id != current_match:\n",
    "            current_match = match_id\n",
    "            # Reset current set for a new match\n",
    "            current_set = None\n",
    "        \n",
    "        # Check for changes in sets\n",
    "        new_set = row['Set1'] + row['Set2'] + 1  # Alternatively, check the actual set being played\n",
    "        if new_set != current_set:\n",
    "            current_set = new_set\n",
    "            set_ids[current_set] = f\"{match_id}_Set{current_set}\"\n",
    "        \n",
    "        # Check for changes in games\n",
    "        if (current_set, game_number) not in game_ids:\n",
    "            game_ids[(current_set, game_number)] = f\"{match_id}_Set{current_set}_Game{game_number}\"\n",
    "        \n",
    "        # Assign identifiers\n",
    "        pt_id = f\"{match_id}_Pt{pt_number}\"\n",
    "        gm_id = game_ids.get((current_set, game_number), None)\n",
    "        set_id = set_ids.get(current_set, None)\n",
    "        \n",
    "        df.at[idx, 'pt_id'] = pt_id\n",
    "        df.at[idx, 'gm_id'] = gm_id\n",
    "        df.at[idx, 'set_id'] = set_id\n",
    "\n",
    "    return df"
   ]
