    "import pandas as pd\n",
    "\n",
    "def parse_shots(df):\n",
    "    # Define character sets for different shot attributes\n",
    "    shot_types = set('fbrsvzopuylmhikqtS')\n",
    "    directions = set('123')\n",
    "    error_types = set('nwdxg!e')\n",
    "    outcome_types = set('*#@')\n",
    "    depth_types = set('789')\n",
    "    serve_zones = set('456')\n",
    "\n",
    "    def parse_code(code):\n",
    "        shots = []\n",
    "        shot = {'shot_num': 1, 'shot_type': '', 'direction': '', 'error_type': '', 'outcome': '', 'depth': '', 'position': '', 'net_cord': '', 'approach': '', 'serve_and_volley': False, 'serve_zone': ''}\n",
    "        i = 0\n",
    "        \n",
    "        while i < len(code):\n",
    "            char = code[i]\n",
    "            \n",
    "            if char in serve_zones:\n",
    "                shot = {'shot_num': 1, 'shot_type': 'S', 'direction': '', 'error_type': '', 'outcome': '', 'depth': '', 'position': '', 'net_cord': '', 'approach': '', 'serve_and_volley': False, 'serve_zone': char}\n",
    "            \n",
    "            if char in shot_types:\n",
    "                shots.append(shot)\n",
    "                shot = {'shot_num': len(shots) + 1, 'shot_type': char, 'direction': '', 'error_type': '', 'outcome': '', 'depth': '', 'position': '', 'net_cord': '', 'approach': '', 'serve_and_volley': False, 'serve_zone': ''}\n",
    "                \n",
    "            elif char in directions:\n",
    "                shot['direction'] = char\n",
    "\n",
    "            elif char in error_types:\n",
    "                shot['error_type'] = char\n",
    "\n",
    "            elif char in outcome_types:\n",
    "                shot['outcome'] = char\n",
    "\n",
    "            elif char in depth_types:\n",
    "                shot['depth'] = char\n",
    "\n",
    "            elif char == '+':\n",
    "                shot['approach'] = '+'\n",
    "\n",
    "            elif char == '-':\n",
    "                shot['position'] = '-'\n",
    "\n",
    "            elif char == '=':\n",
    "                shot['position'] = '='\n",
    "\n",
    "            elif char == ';':\n",
    "                shot['net_cord'] = ';'\n",
    "\n",
    "            elif char == 'c':\n",
    "                shot['serve_and_volley'] = True\n",
    "\n",
    "            i += 1\n",
    "        \n",
    "        if shot['shot_type']:\n",
    "            shots.append(shot)\n",
    "\n",
    "        return shots\n",
    "\n",
    "    data = {col: [] for col in df.columns}\n",
    "    extra_columns = ['serve_zone', 'shot_type', 'direction', 'error_type', 'outcome', 'depth', 'position', 'net_cord', 'approach', 'serve_and_volley', 'shot_num']\n",
    "    for col in extra_columns:\n",
    "        data[col] = []\n",
    "\n",
    "    for index, row in df.iterrows():\n",
    "        for col in ['1st', '2nd']:\n",
    "            if pd.notna(row[col]):\n",
    "                shots = parse_code(row[col])\n",
    "                for shot in shots:\n",
    "                    for df_col in df.columns:\n",
    "                        data[df_col].append(row[df_col])\n",
    "                    data['serve_zone'].append(shot['serve_zone'])\n",
    "                    data['shot_type'].append(shot['shot_type'])\n",
    "                    data['direction'].append(shot['direction'])\n",
    "                    data['error_type'].append(shot['error_type'])\n",
    "                    data['outcome'].append(shot['outcome'])\n",
    "                    data['depth'].append(shot['depth'])\n",
    "                    data['position'].append(shot['position'])\n",
    "                    data['net_cord'].append(shot['net_cord'])\n",
    "                    data['approach'].append(shot['approach'])\n",
    "                    data['serve_and_volley'].append(shot['serve_and_volley'])\n",
    "                    data['shot_num'].append(shot['shot_num'])\n",
    "\n",
    "    return pd.DataFrame(data)"
