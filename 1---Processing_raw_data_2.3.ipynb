    "def add_shot_in_column(df):\n",
    "    # Ensure that PtWinner_Player column is available\n",
    "    df['PtWinner_Player'] = df.apply(lambda row: row['Player_1'] if row['PtWinner'] == 1 else row['Player_2'], axis=1)\n",
    "    \n",
    "    # Initialize the 'shot_in' column as 1 (meaning all shots are in by default)\n",
    "    df['shot_in'] = 1\n",
    "\n",
    "    # Group by both match_id and Pt to ensure the logic applies to each point within each match\n",
    "    for (match_id, pt), group in df.groupby(['match_id', 'Pt']):\n",
    "        first_shot_index = group.index[0]\n",
    "        second_shot_index = first_shot_index + 1 if len(group) > 1 else None\n",
    "        \n",
    "        # Check if the first serve is in or out\n",
    "        if group.at[first_shot_index, '1stIn'] == 0:\n",
    "            # First serve is out\n",
    "            df.at[first_shot_index, 'shot_in'] = 0\n",
    "            \n",
    "            # Check if second serve exists and whether it's in or out\n",
    "            if second_shot_index in group.index and group.at[second_shot_index, 'shot_num'] == 1:\n",
    "                if group.at[second_shot_index, '2ndIn'] == 0:\n",
    "                    # Second serve is out, double fault\n",
    "                    df.at[second_shot_index, 'shot_in'] = 0\n",
    "                else:\n",
    "                    # Second serve is in\n",
    "                    df.at[second_shot_index, 'shot_in'] = 1\n",
    "\n",
    "        # If the first serve is in\n",
    "        else:\n",
    "            df.at[first_shot_index, 'shot_in'] = 1\n",
    "\n",
    "        # Handle the rest of the rally after the serve(s)\n",
    "        last_shot_index = group.index[-1]\n",
    "        last_hitting_player = group.loc[last_shot_index, 'Hitting_Player']\n",
    "        pt_winner_player = group.loc[last_shot_index, 'PtWinner_Player']\n",
    "\n",
    "        # Mark the last shot of the rally as out if the loser of the point hit it\n",
    "        if last_hitting_player != pt_winner_player:\n",
    "            df.at[last_shot_index, 'shot_in'] = 0\n",
    "    \n",
    "    return df\n"
   ]
