    "def determine_winners(df):\n",
    "    # Ensure PtWinner_Player column is available\n",
    "    df['PtWinner_Player'] = df.apply(lambda row: row['Player_1'] if row['PtWinner'] == 1 else row['Player_2'], axis=1)\n",
    "    \n",
    "    # Initialize columns for winners\n",
    "    df['GmWinner_Player'] = None\n",
    "    df['SetWinner_Player'] = None\n",
    "    df['MatchWinner_Player'] = None\n",
    "    \n",
    "    # Process match by match\n",
    "    for match_id, match_df in df.groupby('match_id'):\n",
    "        # Determine Match Winner from the last point of the match\n",
    "        match_winner = match_df['PtWinner_Player'].iloc[-1]\n",
    "        df.loc[match_df.index, 'MatchWinner_Player'] = match_winner\n",
    "        \n",
    "        # Process set by set within the match\n",
    "        for set_id, set_df in match_df.groupby('set_id'):\n",
    "            # Determine Set Winner from the last point of the set\n",
    "            set_winner = set_df['PtWinner_Player'].iloc[-1]\n",
    "            df.loc[set_df.index, 'SetWinner_Player'] = set_winner\n",
    "            \n",
    "            # Process game by game within the set\n",
    "            for gm_id, gm_df in set_df.groupby('gm_id'):\n",
    "                # Determine Game Winner from the last point of the game\n",
    "                game_winner = gm_df['PtWinner_Player'].iloc[-1]\n",
    "                df.loc[gm_df.index, 'GmWinner_Player'] = game_winner\n",
    "\n",
    "    return df"
   ]
