    "def split_pts_column(df):\n",
    "    \n",
    "    def assign_points(row):\n",
    "        scores = row['Pts'].split('-')\n",
    "        \n",
    "        if len(scores) != 2:\n",
    "            return pd.Series([None, None], index=['Player_1_Pts', 'Player_2_Pts'])\n",
    "        \n",
    "        player_1_pts, player_2_pts = str(scores[0]), str(scores[1])\n",
    "        \n",
    "        if row['Svr'] == 2:\n",
    "            player_1_pts, player_2_pts = player_2_pts, player_1_pts\n",
    "            \n",
    "        return pd.Series([player_1_pts, player_2_pts], index=['Player_1_Pts', 'Player_2_Pts'])\n",
    "    \n",
    "    df[['Player_1_Pts', 'Player_2_Pts']] = df.apply(assign_points, axis=1)\n",
    "\n",
    "    # Move Player_1_Pts and Player_2_Pts to indices 7 and 8\n",
    "    cols = df.columns.tolist()  # Get the current column order\n",
    "    # Remove the columns from their current positions\n",
    "    cols.remove('Player_1_Pts')\n",
    "    cols.remove('Player_2_Pts')\n",
    "    \n",
    "    # Insert Player_1_Pts and Player_2_Pts at indices 7 and 8\n",
    "    cols.insert(7, 'Player_1_Pts')\n",
    "    cols.insert(8, 'Player_2_Pts')\n",
    "    \n",
    "    # Reorder the DataFrame\n",
    "    df = df[cols]\n",
    "\n",
    "    return df"
   ]
